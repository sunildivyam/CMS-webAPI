  "homepage": "https://github.com/sunildivyam/raiweb-browserify-gulp-angular",
  "dependencies": {
    "angular": "^1.5.6",
    "angular-animate": "^1.5.7",
    "angular-cookies": "^1.5.8",
    "angular-daterangepicker": "^0.2.3-alpha1",
    "angular-mock": "^1.0.0",
    "angular-sanitize": "^1.5.6",
    "angular-ui-bootstrap": "^2.2.0",
    "angular-ui-router": "^0.3.0",
    "bootstrap": "^3.3.6",
    "bootstrap-social": "^5.0.0",
    "ckeditor": "^4.6.1",
    "dotdotdot": "^1.7.0",
    "font-awesome": "^4.6.3",
    "highlight.js": "^9.9.0",
    "isotope-layout": "^3.0.1",
    "jquery": "^2.2.4",
    "mathjax": "^2.7.0",
    "roboto-fontface": "^0.7.0",
    "tinyscrollbar": "^2.4.2",
    "ui-select": "^0.19.8"
