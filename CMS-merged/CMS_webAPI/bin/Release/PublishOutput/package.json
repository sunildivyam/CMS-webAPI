      "awesome": "node_modules/font-awesome/fonts",
      "roboto": "node_modules/roboto-fontface/fonts/Roboto"
    },
    "imagesDir": "images",
    "pagesDir": "pages",
    "dataDir": "data",
    "stylesDir": "styles",
    "less": {
      "paths": [
        "node_modules/bootstrap/less",
        "node_modules/bootstrap-social",
        "node_modules/font-awesome/less"
      ],
      "cssFileName": "main",
      "appIncludesDir":  "includes"
    }
  },
